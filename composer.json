{
    "name": "schibsted/account-sdk-php",
    "description": "OAuth2 client for Schibsted account",
    "type": "library",
    "license": "MIT",
    "keywords": ["oauth2", "client", "authorization", "schibsted"],
    "require": {
        "league/oauth2-client": "^2.7"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Schibsted\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schibsted\\OAuth2\\Client\\Test\\Provider\\": "tests/"
        }
    }
}
